<!--定义模板-->
<template name="a">
    <view class="page__hd">
    <text class="page__title">{{index}}:{{msg}}</text>
    </view>
</template>
<template name="b">
    <view class="page__hd">
    <text class="page__title">Time:{{time}}</text>
    </view>
</template>
<view class="page">
  <view class="page__hd">
    <text class="page__title">模板用法</text>
  </view>
  <template is="a" data="{{...item}}">
</template>
  <template is="b" data="{{...item}}">
</template>
</view>